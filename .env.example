# GitHub Container Registry username/organization
GITHUB_USER=your-username

# W9 Tools Configuration
W9_TOOLS_BASE_URL=https://w9.se
W9_TOOLS_PORT=10105
W9_TOOLS_FRONTEND_PORT=8081
W9_TOOLS_EMAIL_FROM=W9 Tools <no-reply@w9.se>
W9_TOOLS_PASSWORD_RESET_URL=https://w9.se/reset-password
W9_TOOLS_VERIFICATION_URL=https://w9.se/verify
W9_TOOLS_TURNSTILE_SECRET=your-turnstile-secret
